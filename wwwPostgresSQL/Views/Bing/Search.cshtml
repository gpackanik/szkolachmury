@model IEnumerable<wwwPostgresSQL.Models.SearchResult>

@{
    ViewData["Title"] = "Search";
}

<form asp-action="Search" style="margin-top:20px;">
    <input name="searchPhase" />
    <button type="submit">Search</button>
</form>

<div class="@(Model.Count() == 0 ? " hidden" : "")">
    <h2>Search Results:</h2>
    @foreach (var item in Model)
    {
    <div>
        <partial name="SearchResult.cshtml" model="item" />
    </div>
    }
</div>
